<script type="text/javascript">
    //These variables are defined in PRBeta.js
    lblGIContentId = '#<%=lblGIContent.ClientID%>';

    publicOrderId = '<%=CurrentOrder == null ?  "" : CurrentOrder.OrderDetail.PublicOrderId.ToString() %>';
    countyId = '<%=CurrentOrder == null ?  "" : CurrentOrder.OrderDetail.CountyId.Value.ToString() %>';
    serviceProviderId = '<%=CurrentOrder == null ?  "" : CurrentOrder.ServiceProviderId.ToString() %>';
    canPowerSearch = '<%=CurrentOrder == null ?  "" : CurrentOrder.ServiceProvider.Profile.CanAccessPowerSearches.ToString() %>';
    lblPowerSearchContentId = '#<%=lblPowerSearchContent.ClientID%>';
    publicQueryString = '<%=PublicQueryString%>';
    hdnNeedToSaveState = '<%=hdnNeedToSaveState.ClientID %>';
    CACHE_ID = '<%=CACHE_ID %>';
    filterDropDownId = '<%=ddlTopFilter.ClientID %>';
    filterDropDownBottomId = '<%=ddlBottomFilter.ClientID %>';
    ddlViewId = '<%=ddlView.ClientID %>';

    function SaveCache() {
        var url = "PropertyReportBeta.aspx?" + publicQueryString + "/SaveCache";
        $.ajax({
            url: url,
            data: $("#Form2").serialize(),
            type: "POST",
            cache: false,
            async: false,
            success: function () {
            },
            error: function (response, status, error) {
                if (response != null && response.responseText.length > 0)
                    if (jQuery.parseJSON(response.responseText).Message == "SESSION_TIMEOUT")
                        location.href = "login.aspx?ReturnUrl=" + window.location.pathname + window.location.search;

            }
        });

        return;

        //var u = $get('<%=btnDeleteRecYes.ClientID %>');
        //u.click();
        <%=Page.ClientScript.GetPostBackEventReference(this.btnDeleteRecYes,"") %>;
    }

</script>

