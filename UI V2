function OnGINameSearchExpand_Error(response, status, error) {
    if (response != null && response.responseText.length > 0)
        if (jQuery.parseJSON(response.responseText).Message == "SESSION_TIMEOUT")
            location.href = "login.aspx?ReturnUrl=" + window.location.pathname + window.location.search;
}
----------------------------------------------------------------------------------------------------
function OnGINameSearchCollapse(sender, eventArgs) {
    var tit = eventArgs.get_item().get_text();
    eventArgs.get_item().set_text(tit.substring(0, tit.indexOf("(")) + " (click to expand details)</span>");
}
-------------------------------------------------------------------------------------------------------

function OnParcelExpand(sender, eventArgs) {
    var tit = eventArgs.get_item().get_text();
    eventArgs.get_item().set_text("<span class='ExpandCollapseHeader'>" + tit.substring(0, tit.indexOf("(")) + " (click to collapse details)</span>");
}
function OnParcelCollapse(sender, eventArgs) {
    var tit = eventArgs.get_item().get_text();
    eventArgs.get_item().set_text("<span class='ExpandCollapseHeader'>" + tit.substring(0, tit.indexOf("(")) + " (click to expand details)</span>");
}
